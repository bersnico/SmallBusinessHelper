<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".BusinessPageActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".BusinessAccountActivity" android:background="#7A03A9F4">

        <TextView
                android:text="@string/email"
                android:layout_width="107dp"
                android:layout_height="36dp" android:id="@+id/textView7"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.555" android:textSize="24sp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.418" app:layout_constraintBottom_toTopOf="@+id/textView12"/>
        <TextView
                android:text="@string/phone"
                android:layout_width="107dp"
                android:layout_height="36dp" android:id="@+id/textView8"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.555" android:textSize="24sp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.27" app:layout_constraintBottom_toTopOf="@+id/textView12"/>
        <TextView
                android:text="@string/favorites"
                android:layout_width="107dp"
                android:layout_height="36dp" android:id="@+id/textView14"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.828" app:layout_constraintBottom_toTopOf="@+id/textView12"/>
        <TextView
                android:text="@string/category"
                android:layout_width="107dp"
                android:layout_height="36dp" android:id="@+id/textView12"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.588"/>
        <TextView
                android:text="@string/location"
                android:layout_width="107dp"
                android:layout_height="36dp" android:id="@+id/textView15"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.052" android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintVertical_bias="0.064" app:layout_constraintTop_toBottomOf="@+id/textView12"/>
        <TextView
                android:text="@string/price"
                android:layout_width="107dp"
                android:layout_height="36dp" android:id="@+id/textView11"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
                app:layout_constraintVertical_bias="0.464" app:layout_constraintBottom_toTopOf="@+id/textView12"
                app:layout_constraintTop_toBottomOf="@+id/textView14"/>
        <TextView
                android:text="@string/business"
                android:layout_width="221dp"
                android:layout_height="42dp" android:id="@+id/textView5"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.084" app:layout_constraintVertical_bias="0.023"
                android:textSize="36sp" android:fontFamily="@font/fenix" android:textColor="#131212"
                android:textStyle="bold"/>
        <RatingBar
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/ratingBar2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.497"
                app:layout_constraintVertical_bias="0.342"/>
        <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" tools:src="@tools:sample/avatars" android:id="@+id/imageView"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="84dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="519dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginEnd="267dp"
                android:layout_marginRight="267dp" app:layout_constraintEnd_toEndOf="parent"
                android:contentDescription="@string/profilepic"/>
        <ImageView
                android:layout_width="169dp"
                android:layout_height="213dp" tools:srcCompat="@tools:sample/backgrounds/scenic"
                android:id="@+id/imageView2" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="473dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="45dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginEnd="226dp"
                android:layout_marginRight="226dp" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.404" app:layout_constraintVertical_bias="0.478"
                android:contentDescription="@string/uploaded1"/>
        <ImageView
                android:layout_width="169dp"
                android:layout_height="213dp" tools:srcCompat="@tools:sample/backgrounds/scenic"
                android:id="@+id/imageView4" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="473dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="45dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="217dp" android:layout_marginStart="217dp" android:layout_marginEnd="25dp"
                android:layout_marginRight="25dp" app:layout_constraintEnd_toEndOf="parent"
                android:contentDescription="@string/uploaded2"/>
        <Button
                android:text="@string/return_to_search"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/button"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="667dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="123dp" android:layout_marginStart="123dp" android:layout_marginEnd="123dp"
                android:layout_marginRight="123dp" app:layout_constraintEnd_toEndOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".BusinessAccountActivity" android:background="#7A03A9F4">

    <TextView
            android:text="TextView"
            android:layout_width="165dp"
            android:layout_height="41dp" android:id="@+id/textView7" tools:layout_editor_absoluteY="159dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.666"/>
    <TextView
            android:text="@string/favorites"
            android:layout_width="107dp"
            android:layout_height="36dp" android:id="@+id/textView14"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.838" app:layout_constraintBottom_toTopOf="@+id/textView12"/>
    <TextView
            android:text="@string/price"
            android:layout_width="107dp"
            android:layout_height="36dp" android:id="@+id/textView11"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
            app:layout_constraintVertical_bias="0.459" app:layout_constraintBottom_toTopOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/textView14"/>
    <TextView
            android:text="Category"
            android:layout_width="107dp"
            android:layout_height="36dp" android:id="@+id/textView12"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.612"/>
    <TextView
            android:text="@string/location"
            android:layout_width="107dp"
            android:layout_height="36dp" android:id="@+id/textView15"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.052" android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.072" app:layout_constraintTop_toBottomOf="@+id/textView12"/>
    <TextView
            android:text="@string/your_account"
            android:layout_width="221dp"
            android:layout_height="42dp" android:id="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.084" app:layout_constraintVertical_bias="0.023"
            android:textSize="36sp" android:fontFamily="@font/fenix" android:textColor="#131212"
            android:textStyle="bold"/>
    <RatingBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ratingBar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintHorizontal_bias="0.497"
            app:layout_constraintVertical_bias="0.367"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:src="@tools:sample/avatars" android:id="@+id/imageView"
            tools:layout_editor_absoluteY="84dp" tools:layout_editor_absoluteX="16dp"/>
    <Button
            android:text="@string/edit_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button" tools:layout_editor_absoluteY="646dp"
            tools:layout_editor_absoluteX="145dp"/>
    <TextView
            android:text="TextView"
            android:layout_width="165dp"
            android:layout_height="41dp" android:id="@+id/textView6" tools:layout_editor_absoluteY="84dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.666"/>

<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".BusinessAccountActivity" android:background="#7A03A9F4">

    <TextView
            android:text="@android:string/ok"
            android:layout_width="111dp"
            android:layout_height="34dp" android:id="@+id/textView16"
            android:fontFamily="@font/fenix" android:textSize="24sp"
            app:layout_constraintStart_toEndOf="@+id/textView11" app:layout_constraintTop_toBottomOf="@+id/textView13"
            app:layout_constraintBottom_toTopOf="@+id/textView12" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.031" app:layout_constraintVertical_bias="0.533"/>
    <TextView
            android:text="@string/contact_info"
            android:layout_width="158dp"
            android:layout_height="40dp" android:id="@+id/textView7"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.703" android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.436" app:layout_constraintBottom_toTopOf="@+id/textView12"
            android:fontFamily="@font/fenix"/>
    <TextView
            android:text="@string/business_name"
            android:layout_width="154dp"
            android:layout_height="44dp" android:id="@+id/textView6"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.692" android:textSize="18sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.257" app:layout_constraintBottom_toTopOf="@+id/textView12"
            android:fontFamily="@font/fenix"/>
    <TextView
            android:text="@string/favss"
            android:layout_width="121dp"
            android:layout_height="32dp" android:id="@+id/textView14"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.074" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.827" app:layout_constraintBottom_toTopOf="@+id/textView12"
            android:fontFamily="@font/fenix"/>
    <TextView
            android:text="@string/category"
            android:layout_width="279dp"
            android:layout_height="39dp" android:id="@+id/textView12"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.128" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.588" android:fontFamily="@font/fenix"/>
    <TextView
            android:text="@string/location"
            android:layout_width="278dp"
            android:layout_height="61dp" android:id="@+id/textView15"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.136" android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.053" app:layout_constraintTop_toBottomOf="@+id/textView12"
            android:fontFamily="@font/fenix"/>
    <TextView
            android:text="@string/price"
            android:layout_width="118dp"
            android:layout_height="34dp" android:id="@+id/textView11"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.081" android:textSize="24sp"
            app:layout_constraintVertical_bias="0.548" app:layout_constraintBottom_toTopOf="@+id/textView12"
            app:layout_constraintTop_toBottomOf="@+id/textView14" android:fontFamily="@font/fenix"/>
    <TextView
            android:text="@string/your_account"
            android:layout_width="221dp"
            android:layout_height="42dp" android:id="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.084" app:layout_constraintVertical_bias="0.023"
            android:textSize="36sp" android:fontFamily="@font/fenix" android:textColor="#131212"
            android:textStyle="bold"/>
    <RatingBar
            android:layout_width="248dp"
            android:layout_height="58dp" android:id="@+id/ratingBar3"
            android:isIndicator="true"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView7"
            app:layout_constraintBottom_toTopOf="@+id/textView14" app:layout_constraintHorizontal_bias="0.515"
            app:layout_constraintVertical_bias="0.788"/>
    <ImageView
            android:layout_width="150dp"
            android:layout_height="150dp" android:src="@drawable/ic_launcher_background" android:id="@+id/imageView"
            android:contentDescription="@string/profilepic"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toStartOf="@+id/textView6"
            app:layout_constraintTop_toBottomOf="@+id/textView5" android:layout_marginTop="12dp"
            app:layout_constraintHorizontal_bias="0.54" app:layout_constraintBottom_toTopOf="@+id/ratingBar3"
            app:layout_constraintVertical_bias="0.093" android:scaleType="fitXY"/>
    <Button
            android:text="@string/edit_profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/edit"
            android:textColorLink="#FFFFFF" android:fontFamily="@font/fenix"
            app:layout_constraintTop_toBottomOf="@+id/textView15"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"/>
    <Button
            android:text="@string/log_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2"
            android:fontFamily="@font/fenix"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toTopOf="@+id/textView6" app:layout_constraintStart_toEndOf="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.541"
            app:layout_constraintVertical_bias="0.355"/>
    <TextView
            android:text="@android:string/ok"
            android:layout_width="111dp"
            android:layout_height="34dp" android:id="@+id/textView13"
            app:layout_constraintTop_toBottomOf="@+id/ratingBar3"
            app:layout_constraintBottom_toTopOf="@+id/textView11" app:layout_constraintStart_toEndOf="@+id/textView14"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintVertical_bias="0.552" android:fontFamily="@font/fenix" android:textSize="24sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
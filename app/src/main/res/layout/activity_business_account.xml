            android:layout_marginBottom="45dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="16dp" android:layout_marginStart="16dp" android:layout_marginEnd="226dp"
            android:layout_marginRight="226dp" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0" app:layout_constraintVertical_bias="0.0"
            android:contentDescription="@string/uploaded1"/>
    <ImageView
            android:layout_width="169dp"
            android:layout_height="213dp" tools:srcCompat="@tools:sample/backgrounds/scenic"
            android:id="@+id/imageView4" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="473dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="45dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="217dp" android:layout_marginStart="217dp" android:layout_marginEnd="25dp"
            android:layout_marginRight="25dp" app:layout_constraintEnd_toEndOf="parent"
            android:contentDescription="@string/uploaded2"/>
    <Button
            android:text="@string/log_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button2"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="667dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="296dp" android:layout_marginStart="296dp" android:layout_marginEnd="25dp"
            android:layout_marginRight="25dp" app:layout_constraintEnd_toEndOf="parent"/>

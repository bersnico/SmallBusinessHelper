<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".BusinessAccountActivity" android:background="#7A03A9F4">

    <EditText
            android:layout_width="173dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:text="Contact Info"
            android:ems="10"
            android:id="@+id/editTextTextPersonName2" tools:layout_editor_absoluteY="164dp"
            tools:layout_editor_absoluteX="164dp" android:textSize="24sp"/>
    <TextView
            android:text="Favorites"
            android:layout_width="107dp"
            android:layout_height="36dp" android:id="@+id/textView14"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.424"/>
    <TextView
            android:text="Price"
            android:layout_width="107dp"
            android:layout_height="36dp" android:id="@+id/textView11"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.499"/>
    <TextView
            android:text="Category"
            android:layout_width="107dp"
            android:layout_height="36dp" android:id="@+id/textView12"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.583"/>
    <TextView
            android:text="Location"
            android:layout_width="107dp"
            android:layout_height="36dp" android:id="@+id/textView15"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.042" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.664"/>
    <TextView
            android:text="@string/your_account"
            android:layout_width="221dp"
            android:layout_height="42dp" android:id="@+id/textView5"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.084" app:layout_constraintVertical_bias="0.023"
            android:textSize="36sp" android:fontFamily="@font/fenix" android:textColor="#131212"
            android:textStyle="bold"/>
    <RatingBar
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/ratingBar" tools:layout_editor_absoluteY="247dp"
            tools:layout_editor_absoluteX="16dp"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" tools:src="@tools:sample/avatars" android:id="@+id/imageView"
            tools:layout_editor_absoluteY="84dp" tools:layout_editor_absoluteX="16dp"/>
    <Button
            android:text="Edit Profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/button" tools:layout_editor_absoluteY="733dp"
            tools:layout_editor_absoluteX="22dp"/>
    <EditText
            android:layout_width="173dp"
            android:layout_height="48dp"
            android:inputType="textPersonName"
            android:text="Business Name"
            android:ems="10"
            android:id="@+id/editTextTextPersonName" tools:layout_editor_absoluteY="93dp"
            tools:layout_editor_absoluteX="164dp" android:textSize="24sp"/>
</androidx.constraintlayout.widget.ConstraintLayout>
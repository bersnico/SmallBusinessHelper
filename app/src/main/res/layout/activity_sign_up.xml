<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SignUpActivity" android:background="#7703A9F4">

    <TextView
            android:text="@string/sign_up"
            android:layout_width="113dp"
            android:layout_height="53dp" android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.196" app:layout_constraintHorizontal_bias="0.498"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:fontFamily="monospace"
            android:textStyle="bold"/>
    <EditText
            android:layout_width="327dp"
            android:layout_height="45dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword"
            android:hint="@string/enter_password" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:autofillHints=""
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress"
            app:layout_constraintVertical_bias="0.041"/>
    <EditText
            android:layout_width="329dp"
            android:layout_height="46dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword" app:layout_constraintVertical_bias="0.027"
            android:hint="@string/re_enter_password" android:autofillHints=""/>
    <EditText
            android:layout_width="329dp"
            android:layout_height="55dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAddress"
            android:autofillHints="" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.044"
            android:hint="@string/enter_email"/>
    <CheckBox
            android:text="@string/select_if_business_account"
            android:layout_width="330dp"
            android:layout_height="43dp" android:id="@+id/checkBox"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2"
            app:layout_constraintVertical_bias="0.032" app:layout_constraintHorizontal_bias="0.525"
            app:buttonTint="#132527"/>
    <EditText
            android:layout_width="338dp"
            android:layout_height="45dp"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/editTextPhone"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/checkBox"
            app:layout_constraintVertical_bias="0.059" android:hint="@string/enter_phone_number"
            android:autofillHints=""/>
    <EditText
            android:layout_width="320dp"
            android:layout_height="38dp"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:id="@+id/editTextTextPostalAddress"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextPhone" app:layout_constraintVertical_bias="0.102"
            android:hint="@string/enter_business_address" android:autofillHints=""/>
</androidx.constraintlayout.widget.ConstraintLayout>
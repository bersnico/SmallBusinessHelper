<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".SignUpActivity" android:background="#7703A9F4" android:id="@+id/signUp">

    <EditText
            android:layout_width="302dp"
            android:layout_height="49dp"
            android:ems="10"
            android:id="@+id/editTextPriceRange"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextCategory"
            app:layout_constraintVertical_bias="0.11"
            android:hint="@string/enter_price_range_1_4" android:autofillHints="" android:inputType="textPersonName"
            app:layout_constraintHorizontal_bias="0.495" android:cursorVisible="true"
            android:fadingEdge="horizontal|vertical" android:visibility="invisible" android:background="#36594B4B"
            android:fontFamily="@font/fenix"/>
    <EditText
            android:layout_width="302dp"
            android:layout_height="49dp"
            android:ems="10"
            android:id="@+id/editTextBusinessName2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextPhone"
            app:layout_constraintVertical_bias="0.049"
            android:hint="@string/enter_business_name" android:autofillHints="" android:inputType="textPersonName"
            app:layout_constraintHorizontal_bias="0.495" android:cursorVisible="true"
            android:fadingEdge="horizontal|vertical" android:visibility="invisible" android:background="#36594B4B"
            android:fontFamily="@font/fenix"/>
    <TextView
            android:text="@string/sign_up"
            android:layout_width="113dp"
            android:layout_height="53dp" android:id="@+id/textView"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.039"
            android:textAppearance="@style/TextAppearance.AppCompat.Large" android:fontFamily="@font/fenix"
            android:textStyle="bold" android:fadingEdge="horizontal|vertical" android:gravity="center"/>
    <EditText
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword"
            android:hint="@string/enter_password" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:autofillHints=""
            app:layout_constraintTop_toBottomOf="@+id/editTextTextEmailAddress"
            app:layout_constraintVertical_bias="0.041" android:cursorVisible="true"
            android:fadingEdge="horizontal|vertical" android:background="#36594B4B" android:fontFamily="@font/fenix"/>
    <EditText
            android:layout_width="300dp"
            android:layout_height="46dp"
            android:inputType="textPassword"
            android:ems="10"
            android:id="@+id/editTextTextPassword2"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword" app:layout_constraintVertical_bias="0.027"
            android:hint="@string/reenter_password" android:autofillHints="" android:cursorVisible="true"
            android:fadingEdge="horizontal|vertical" android:background="#36594B4B" android:fontFamily="@font/fenix"/>
    <EditText
            android:layout_width="302dp"
            android:layout_height="49dp"
            android:ems="10"
            android:id="@+id/editTextCategory"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextBusinessName2"
            app:layout_constraintVertical_bias="0.078"
            android:hint="@string/enter_a_category" android:autofillHints="" android:inputType="textPersonName"
            app:layout_constraintHorizontal_bias="0.495" android:cursorVisible="true"
            android:fadingEdge="horizontal|vertical" android:visibility="invisible" android:background="#36594B4B"
            android:fontFamily="@font/fenix"/>
    <EditText
            android:layout_width="300dp"
            android:layout_height="55dp"
            android:inputType="textEmailAddress"
            android:ems="10"
            android:id="@+id/editTextTextEmailAddress"
            android:autofillHints="" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.044"
            android:hint="@string/enter_email" android:cursorVisible="true" android:fadingEdge="horizontal|vertical"
            android:background="#36594B4B" android:fontFamily="@font/fenix"/>
    <CheckBox
            android:text="@string/select_if_business_account"
            android:layout_width="300dp"
            android:layout_height="43dp" android:id="@+id/checkBox"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPostalAddress"
            app:layout_constraintVertical_bias="0.038" app:layout_constraintHorizontal_bias="0.504"
            app:buttonTint="#4C91AB" android:textColor="#46869E"
            android:fadingEdge="horizontal|vertical" android:background="#36594B4B" android:fontFamily="@font/fenix"/>
    <EditText
            android:layout_width="300dp"
            android:layout_height="45dp"
            android:inputType="phone"
            android:ems="10"
            android:id="@+id/editTextPhone"
            app:layout_constraintStart_toStartOf="parent" app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintTop_toBottomOf="@+id/checkBox"
            app:layout_constraintVertical_bias="0.038" android:hint="@string/enter_phone_number"
            android:autofillHints="" android:cursorVisible="true" android:fadingEdge="horizontal|vertical"
            android:visibility="invisible" android:background="#36594B4B" app:layout_constraintHorizontal_bias="0.495"
            android:fontFamily="@font/fenix"/>
    <EditText
            android:layout_width="300dp"
            android:layout_height="49dp"
            android:inputType="textPostalAddress"
            android:ems="10"
            android:id="@+id/editTextTextPostalAddress"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextTextPassword2" app:layout_constraintVertical_bias="0.037"
            android:hint="@string/enter_town" android:autofillHints="" android:cursorVisible="true"
            android:fadingEdge="horizontal|vertical" android:visibility="visible" android:background="#36594B4B"
            app:layout_constraintHorizontal_bias="0.504" android:fontFamily="@font/fenix"/>
    <Button
            android:text="@string/sign_up"
            android:layout_width="188dp"
            android:layout_height="72dp" android:id="@+id/signUpButton"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editTextCategory"
            app:layout_constraintBottom_toBottomOf="parent" android:textSize="24sp"
            app:layout_constraintHorizontal_bias="0.497" app:layout_constraintVertical_bias="0.774"
            android:fontFamily="@font/fenix"/>
    <TextView
            android:text="@string/make_sure_all_fields_have_a_valid_entry"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/fillInAll"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="4dp" app:layout_constraintTop_toBottomOf="@+id/textView"
            app:layout_constraintBottom_toTopOf="@+id/editTextTextEmailAddress" android:fontFamily="@font/fenix"
            android:textColor="#FF0000" android:visibility="invisible"/>
</androidx.constraintlayout.widget.ConstraintLayout>
<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ConsumerAccountActivity">
    <Button
            android:text="@string/log_out"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:id="@+id/logout"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginBottom="667dp" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginLeft="296dp" android:layout_marginStart="296dp" android:layout_marginEnd="25dp"
            android:layout_marginRight="25dp" app:layout_constraintEnd_toEndOf="parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".BusinessAccountActivity" android:background="#7A03A9F4" android:id="@+id/cAcctPg"
            tools:layout_editor_absoluteY="107dp" tools:layout_editor_absoluteX="-16dp">

        <TextView
                android:text="@string/contact_info"
                android:layout_width="250dp"
                android:layout_height="36dp" android:id="@+id/textView7"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.975" android:textSize="24sp"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="116dp"/>
        <TextView
                android:text="@string/location"
                android:layout_width="107dp"
                android:layout_height="36dp" android:id="@+id/textView15"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintHorizontal_bias="0.0" android:textSize="24sp"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="396dp" app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="299dp" android:layout_marginLeft="16dp" android:layout_marginStart="16dp"
                android:layout_marginEnd="273dp" android:layout_marginRight="273dp"
                app:layout_constraintVertical_bias="0.0"/>
        <TextView
                android:text="@string/your_account"
                android:layout_width="221dp"
                android:layout_height="42dp" android:id="@+id/textView5"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHorizontal_bias="0.084" app:layout_constraintVertical_bias="0.023"
                android:textSize="36sp" android:fontFamily="@font/fenix" android:textColor="#131212"
                android:textStyle="bold"/>
        <ImageView
                android:layout_width="150dp"
                android:layout_height="150dp" tools:src="@tools:sample/avatars" android:id="@+id/imageView"
                android:contentDescription="@string/todo"
                app:layout_constraintBottom_toTopOf="@+id/textView15"
                app:layout_constraintTop_toBottomOf="@+id/textView5" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/textView7" app:layout_constraintHorizontal_bias="1.0"
                app:layout_constraintVertical_bias="0.307"/>
        <Button
                android:text="@string/edit_profile"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" android:id="@+id/edit"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="667dp" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="16dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginLeft="145dp" android:layout_marginStart="145dp" android:layout_marginEnd="146dp"
                android:layout_marginRight="146dp" app:layout_constraintEnd_toEndOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>